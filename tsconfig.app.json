{
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "types": [
      "unplugin-vue-router/client"
    ]
  },
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "exclude": [
    "src/**/__tests__/*"
  ],
  "vueCompilerOptions": {
    "target": 3
  },
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "./src/typed-router.d.ts",
    "auto-imports.d.ts"
  ]
}